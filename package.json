{
  "name": "personality-trait-names",
  "version": "2.0.4",
  "description": "Obtain trait names for IBM Watson Personality Insights traits.",
  "main": "lib/index.js",
  "scripts": {
    "compile": "browserify -t [ babelify --presets [ es2015 ] ] --standalone PersonalityTraitNames lib/index.js -o dist/index.js",
    "prepublish": "npm run compile",
    "test": "npm run lint && npm run codecov",
    "lint": "eslint .",
    "autofix": "eslint . --fix",
    "codecov": "istanbul cover mocha test && codecov"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/personality-insights/trait-names"
  },
  "keywords": [
    "trait",
    "names",
    "personality",
    "personality insights",
    "translated",
    "i18n"
  ],
  "author": "IBM Corp.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/personality-insights/trait-names/issues"
  },
  "homepage": "https://github.com/personality-insights/trait-names#readme",
  "dependencies": {
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.11.1",
    "istanbul": "^0.4.3",
    "mocha": "^3.2.0"
  }
}
